      "type": "relation",
      "relation": "oneToOne",
      "target": "api::docenti.docenti"
    },
    "oraInizio": {
      "type": "time",
      "required": true,
      "default": "09:00:00.000"
    },
    "oraFine": {
      "type": "time",
      "required": true,
      "unique": false
    },
    "semestre": {
      "type": "enumeration",
      "enum": [
