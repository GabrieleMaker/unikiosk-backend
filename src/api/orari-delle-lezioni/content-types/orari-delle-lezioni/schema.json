        "Lunedi",
        "Martedi",
        "Mercoledi",
        "Giovedi",
        "Venerdi",
        "Sabato",
        "Domenica"
      ]
    },
    "corso": {
      "type": "string",
      "required": true
    },
    "codiceCorso": {
      "type": "string"
    },
